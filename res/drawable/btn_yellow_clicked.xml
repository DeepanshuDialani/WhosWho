<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>
            <solid
                android:color="#f3ae1b" />

            <padding
                
                android:top="5dp"
                
                android:bottom="5dp" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient
                android:startColor="#0000"
                android:endColor="#0000"
                android:angle="0" />
            <stroke
                android:width="1dp"
                android:color="#918E90" />
            <padding
                
                android:top="5dp"
                
                android:bottom="5dp" />
        </shape>
    </item>
</selector>